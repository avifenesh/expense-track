    "react-dom": "^18",
    "react-hook-form": "^7.64.0",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@playwright/test": "^1.51.1",
