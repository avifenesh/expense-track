{
  "name": "expense-track",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "npm run prisma:generate && next build",
    "start": "node scripts/start.js",
    "lint": "eslint . --max-warnings 0",
    "prisma:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate deploy",
    "db:seed": "prisma db seed",
    "migrate:users": "tsx scripts/migrate-to-user-model.ts",
    "rollback:users": "tsx scripts/rollback-user-migration.ts",
    "db:up:local": "docker compose up -d db",
    "db:down:local": "docker compose down",
    "db:logs:local": "docker compose logs -f db",
    "dev:local": "npm run db:up:local && npm run prisma:generate && npm run dev",
    "setup:local": "bash ./scripts/setup-local.sh",
    "test": "vitest run",
    "test:e2e": "playwright test",
    "check-types": "tsc --noEmit",
    "postinstall": "prisma generate",
    "cleanup": "rm -rf .next node_modules/.cache tsconfig.tsbuildinfo test-results .vercel .railway && echo 'âœ“ Cleaned build artifacts and cache'",
    "prepare": "husky"
  },
  "dependencies": {
    "@prisma/adapter-pg": "^7.2.0",
    "@prisma/client": "^7.2.0",
    "@sentry/nextjs": "^10.34.0",
    "@types/jsonwebtoken": "^9.0.10",
    "ai": "^6.0.5",
    "bcryptjs": "^3.0.3",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "jsonwebtoken": "^9.0.3",
    "lucide-react": "^0.562.0",
    "next": "^16.1.1",
    "nodemailer": "^7.0.12",
    "pg": "^8.16.3",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "swagger-ui-react": "^5.31.0",
    "zod": "^4.3.4"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.39.2",
    "@playwright/test": "^1.57.0",
    "@testing-library/react": "^16.3.1",
    "@types/node": "^25",
    "@types/nodemailer": "^7.0.5",
    "@types/pg": "^8.16.0",
    "@types/react": "^19.2.8",
    "@types/react-dom": "^19.2.3",
    "@types/swagger-ui-react": "^4.0.0",
    "@vitest/coverage-v8": "^4.0.17",
    "autoprefixer": "^10",
    "dotenv-cli": "^11.0.0",
    "eslint": "^9",
    "eslint-config-next": "16.1.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "happy-dom": "^20.3.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "magicast": "^0.5.1",
    "openapi-types": "^12.1.3",
    "postcss": "^8",
    "prettier": "^3.7.4",
    "prisma": "^7.2.0",
    "tailwindcss": "^3.4.0",
    "ts-node": "^10.9.2",
    "typescript": "^5",
    "typescript-eslint": "^8.51.0",
    "vitest": "^4.0.17"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"commonjs\"} prisma/seed.ts"
  },
  "lint-staged": {
    "**/*.{ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "**/*.{js,jsx,json,md}": [
      "prettier --write"
    ]
  },
  "overrides": {
    "magicast": "^0.5.1"
  }
}
