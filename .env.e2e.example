# E2E Test Configuration
# Copy to .env.e2e and customize values

# E2E Test User 1 (Primary)
AUTH_USER1_EMAIL=e2e-user1@test.example.com
AUTH_USER1_DISPLAY_NAME=TestUserOne
AUTH_USER1_PASSWORD_HASH="<bcrypt hash of test password>"
AUTH_USER1_PREFERRED_CURRENCY=USD

# E2E Test User 2 (Secondary)
AUTH_USER2_EMAIL=e2e-user2@test.example.com
AUTH_USER2_DISPLAY_NAME=TestUserTwo
AUTH_USER2_PASSWORD_HASH="<bcrypt hash of test password>"
AUTH_USER2_PREFERRED_CURRENCY=EUR

# Session secrets (generate unique values)
AUTH_SESSION_SECRET=<generate with: openssl rand -hex 32>
JWT_SECRET=<generate with: openssl rand -hex 32>
